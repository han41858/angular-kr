                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                <p><code><a href=\"api/core/global/Listener\" class=\"code-anchor\">Listener</a>[]</code>: Array of event listeners on the DOM element.</p>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               \n  \n\n\n  \n\n  \n</div>\n\n\n  \n\n  \n\n\n\n  \n<section class=\"usage-notes\">\n  <h2 id=\"usage-notes\">Usage notes<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/global/ngGetListeners#usage-notes\"><i class=\"material-icons\">link</i></a></h2>\n  <p>Given the following DOM structure:</p>\n<code-example>\n&#x3C;my-app>\n  &#x3C;div (click)=\"doSomething()\">&#x3C;/div>\n&#x3C;/my-app>\n</code-example>\n<p>Calling <code><a href=\"api/core/global/ngGetListeners\" class=\"code-anchor\">getListeners</a></code> on <code>&#x3C;div></code> will return an object that looks as follows:</p>\n<code-example>\n{\n  name: 'click',\n  element: &#x3C;div>,\n  callback: () => doSomething(),\n  useCapture: false\n}\n</code-example>\n\n</section>\n\n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/core/global\n - api/core/global/Listener\n-->\n<!-- links from this doc:\n - /api\n - api/core\n - api/core/global\n - api/core/global/Listener\n - api/core/global/ngGetListeners#nggetlisteners\n - api/core/global/ngGetListeners#usage-notes\n - https://github.com/angular/angular/edit/master/packages/core/src/render3/util/discovery_utils.ts?message=docs(core)%3A%20describe%20your%20change...#L295-L359\n - https://github.com/angular/angular/tree/11.1.0-next.0/packages/core/src/render3/util/discovery_utils.ts#L295-L359\n-->"
